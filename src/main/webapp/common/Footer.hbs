{{#defineBlock "Footer"}}
    {{#defineBlockContainer}}
        {{#defineBlockBody}}
            <section class="wrapper" id="globalFooter">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-6 col-lg-offset-1 col-sm-12 col-xs-12" id="footerLeft">
                            <div class="row">
                                {{titledSections}}
                                <div class="footerColumnn col-xs-12 hidden-sm hidden-md hidden-lg">
                                    <div class="headerFooter">
                                        <a data-target="#contactUsModal" data-toggle="modal">Contact Us</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-lg-offset-1 col-sm-12 col-xs-12" id="footerRight">
                            <div class="row">
                                <div class="col-lg-12 col-sm-6 col-xs-12">
                                    {{#with callToAction}}
                                        <div class="haveQuestions hidden-xs">{{this.title}}</div>
                                        <button class="btn btn-primary hidden-xs hidden-sm hidden-md">
                                            <div>{{this.link.body}}</div>
                                        </button>
                                    {{/with}}
                                    <ul id="socialLinks">
                                        {{#each socialLinks}}
                                            <li id="{{this.body}}">
                                                <a href="{{this.href}}"><span class="{{this.icon}}"></span></a>
                                            </li>
                                        {{/each}}
                                    </ul>
                                </div>
                                <div class="col-lg-12 col-sm-6 col-xs-12">
                                    <div class="footerLinks">
                                        {{#each miscellaneousLinks}}
                                            <a href="{{this.href}}"><span>{{this.body}}</span></a>
                                        {{/each}}
                                    </div>
                                    <div class="copyright">Â© Copyright 2016 TransUnion LLC. All Rights Reserved.
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        {{/defineBlockBody}}
    {{/defineBlockContainer}}
{{/defineBlock}}